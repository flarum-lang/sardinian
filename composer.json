{
	"name": "flarum-lang/sardinian",
	"type": "flarum-extension",
	"description": "Sardinian language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/sardinian",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"sc-locale",
		"sardinian-locale",
		"sardinian"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/sardinian/issues",
		"source": "https://github.com/flarum-lang/sardinian"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Sardinian",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "sc",
			"title": "Sardinian"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
